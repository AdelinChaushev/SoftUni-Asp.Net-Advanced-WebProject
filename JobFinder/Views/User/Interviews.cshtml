@model IEnumerable<UserInterviewOutputViewModel> 
@{
    ViewBag.Tilte = "Your interview schedule";
}
<h1 class="text-center">@ViewBag.Tilte</h1>
@if (Model != null)
{
    

<table class="table table-hover table-dark">
  <thead>
    <tr>      
      <th scope="col">Number</th>
      <th scope="col">Company Name</th>
      <th scope="col">Job Title</th>     
      <th scope="col">Interview Start</th>
      <th scope="col">Interview End</th>
      <th scope="col">Decline Iterview</th>


    </tr>
  </thead>
  <tbody>
    
     @foreach (var item in Model)
            {

             int counter = 1;
             <tr>
             <td>@counter</td>
             <partial name="_UserInterviewPartial" model="item"></partial>
             
             counter++;
              </tr>
            }
   
  </tbody>
</table>
}
